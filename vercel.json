{
  "version": 2,
  "builds": [
    {
      "src": "api/vod/[...path].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/verify.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/data/[table].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/supabase/query.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/vod(.*)",
      "dest": "/api/vod/[...path].js"
    },
    {
      "src": "/api/auth/verify",
      "dest": "/api/auth/verify.js"
    },
    {
      "src": "/api/data/(?<table>[^/]+)",
      "dest": "/api/data/[table].js"
    },
    {
      "src": "/api/supabase/query",
      "dest": "/api/supabase/query.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
